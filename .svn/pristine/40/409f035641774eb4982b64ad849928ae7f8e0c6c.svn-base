    margin-right: 20px;
  }

  
  /************** lecture-form.css  *************/
  #lectureForm select, #lectureForm input {
  	width: 50%;
    height: 50px;
    padding: 0px 0px 0px 20px;
    font-size: 22px;
    text-align: center;
    border: 6px solid rgb(254, 202, 84);
    border-radius: 0px;
  }
 
 #lectureForm .lect-price-form {
 	margin-top: 50px;
 	text-align: center;
 }
 
 #lectureForm .lect-price-form input {
  	width: 38%
  }
  
 #lectureForm .sub-form.category select {
 	width: 31%
 }
 
.hidden {
 	display: none;
 }
  
 input[type="number"]:focus, input[type="file"]:focus  {
 	outline: none;
 }
  
 #lectureForm .price-info {
	margin-bottom: 18px;
 } 
 
 #lectureForm .price-info.discount input {
    height: 50px;
    padding: 0px 0px 0px 20px;
    font-size: 22px;
    text-align: center;
    border: 6px solid rgb(254, 202, 84);
    border-radius: 0px;
 }
  .instructor-banner {
    height: 120px;
    margin-top: 20px;
    margin-bottom: 30px;
  }
  
  #instructor-banner-name {
    margin-top: 30px;
    margin-left: 10px;
  }
  
  #instructor-banner-description {
    margin-left: 10px;
    font-size: 20px;
  }
  
  .title-box {
    border: 6px solid #ff7373;
    background-color: white;
    height: 53px;
    width: 100%;
    color: black;
    font-size: 20px;
    text-align: left;
  }
  
  .title-box::placeholder {
    margin-left: 10px;
