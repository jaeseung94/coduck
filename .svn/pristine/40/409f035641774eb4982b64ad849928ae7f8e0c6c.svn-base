		
.lecture-container {
	text-align: center;
}
.main-title > h1 {
    display: inline-block;
  }
  
  .main-text {
    margin: 0px;
    text-align: center;
  }
  
  .sub-text {
    margin: 5px 0px 0px 0px;
    font-size: 18px;
  }
  
  .instructorName > p {
    display: inline-block;
  }
  
  .main-sort {
    float: right;
    border: 3px solid #ff7373;
    border-radius: 10px;
    width: 180px;
    padding-left: 60px;
    font-size: 16px;
    font-weight: bold;
    align: center;
  }
  
  .lecture-search {
    float: right;
    margin: 20px 800px 20px 50px;
  }
  
  .create-info {
    width: auto;
    height: 80px;
    text-align: center;
    font-size: 40px;
    border: 3px solid #afeeee;
    border-radius: 10px;
    background-size: 100;
  }
  
  .tip-message {
    text-align: center;
    font-size: 20px;
  }
  .sub-form {
  	margin-top: 50px;	
  	text-align: center;
  } 
   
  .sub-title {
    color: rgba(0, 0, 0, 0.25);
	font-size: 22px;
	text-align: center;
  }
  
  input[type=file] {
  	display: inline-block;
  }
  
  .sub-form.file-form h2 {
  	margin-bottom: 20px;
  }
  
  #lectureForm h2 {
  	text-align: center;
  }
  
  .result-tab {
    width: 850px;
    height: 640px;
    border: 1px solid #afeeee;
  }
  
  .result-category {
    float: left;
    color: rgba(0, 0, 0, 0.25);
