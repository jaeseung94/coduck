package kr.co.coduck.vo;

import org.apache.ibatis.type.Alias;

@Alias("CateInfo")
public class CateInfo {
	
	private int no;
	private int qtTotalCnt;	//문항 총개수
	private int ltdTime;	//제한시간
	private int passScore;
	private int cateNo;
	private int testPrice;
	private int name;

	public CateInfo() {}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public int getQtTotalCnt() {
		return qtTotalCnt;
	}

	public void setQtTotalCnt(int qtTotalCnt) {
		this.qtTotalCnt = qtTotalCnt;
	}

	public int getLtdTime() {
		return ltdTime;
	}

	public void setLtdTime(int ltdTime) {
		this.ltdTime = ltdTime;
	}

	public int getPassScore() {
		return passScore;
	}

	public void setPassScore(int passScore) {
		this.passScore = passScore;
	}

	public int getCateNo() {
		return cateNo;
	}

	public void setCateNo(int cateNo) {
		this.cateNo = cateNo;
	}

	public int getTestPrice() {
		return testPrice;
	}

	public void setTestPrice(int testPrice) {
		this.testPrice = testPrice;
	}

	public int getName() {
		return name;
	}

	public void setName(int name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "CateInfo [no=" + no + ", qtTotalCnt=" + qtTotalCnt + ", ltdTime=" + ltdTime + ", passScore=" + passScore
				+ ", cateNo=" + cateNo + ", testPrice=" + testPrice + ", name=" + name + "]";
	}
	
}
