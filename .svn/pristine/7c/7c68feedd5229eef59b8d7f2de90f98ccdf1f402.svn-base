package kr.co.coduck.dto;

import org.apache.ibatis.type.Alias;

@Alias("CartTestDto")
public class CartTestDto {

	private int no;
	private int userNo;
	private String	name;
	private int price;
	private String ep;
	
	public CartTestDto() {
		
	}
	
	

	public int getUserNo() {
		return userNo;
	}

	public void setUserNo(int userNo) {
		this.userNo = userNo;
	}



	public int getNo() {
		return no;
	}



	public void setNo(int no) {
		this.no = no;
	}



	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	public int getPrice() {
		return price;
	}



	public void setPrice(int price) {
		this.price = price;
	}



	public String getEp() {
		return ep;
	}



	public void setEp(String ep) {
		this.ep = ep;
	}

	
}
